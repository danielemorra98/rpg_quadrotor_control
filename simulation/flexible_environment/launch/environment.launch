<launch>

  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />

	<!-- Convert an xacro and put on parameter server -->
	<param name="robot_description" command="$(find xacro)/xacro $(find flexible_environment)/urdf/flexible_window.xacro" />

	<!-- Spawn a robot into Gazebo -->
	<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model window_environment" />

</launch>